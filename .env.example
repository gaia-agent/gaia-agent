# OpenAI API Key (Required)
# Format: sk-...
OPENAI_API_KEY=

# Optional: Custom OpenAI API endpoint
# OPENAI_BASE_URL=https://api.openai.com/v1

# Hugging Face Token (Required for GAIA benchmark)
# Get your token from: https://huggingface.co/settings/tokens
# Format: hf_...
HUGGINGFACE_TOKEN=

# Sandbox Providers (Choose one or both)
# E2B cloud sandbox (recommended)
E2B_API_KEY=

# Sandock API (alternative)
SANDOCK_API_KEY=

# Browser Automation (Choose one or both)
# BrowserUse SDK (recommended)
BROWSERUSE_API_KEY=

# AWS Bedrock AgentCore (alternative)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Search Providers (Choose one or both)
# Tavily AI search (recommended)
TAVILY_API_KEY=

# Exa neural search (alternative)
EXA_API_KEY=

# Memory Providers (Optional)
# Mem0 memory service
MEM0_API_KEY=

# AWS AgentCore Memory uses AWS credentials above
