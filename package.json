{
  "name": "@gaia-agent/sdk",
  "version": "0.1.22",
  "description": "Production-ready AI agent library using AI SDK v6 ToolLoopAgent for GAIA benchmarks with swappable providers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "default": "./dist/types.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.js",
      "default": "./dist/tools/index.js"
    },
    "./tools/core": {
      "types": "./dist/tools/core.d.ts",
      "import": "./dist/tools/core.js",
      "default": "./dist/tools/core.js"
    },
    "./tools/search": {
      "types": "./dist/tools/search/index.d.ts",
      "import": "./dist/tools/search/index.js",
      "default": "./dist/tools/search/index.js"
    },
    "./tools/search/tavily": {
      "types": "./dist/tools/search/tavily.d.ts",
      "import": "./dist/tools/search/tavily.js",
      "default": "./dist/tools/search/tavily.js"
    },
    "./tools/search/openai": {
      "types": "./dist/tools/search/openai.d.ts",
      "import": "./dist/tools/search/openai.js",
      "default": "./dist/tools/search/openai.js"
    },
    "./tools/search/exa": {
      "types": "./dist/tools/search/exa.d.ts",
      "import": "./dist/tools/search/exa.js",
      "default": "./dist/tools/search/exa.js"
    },
    "./tools/sandbox": {
      "types": "./dist/tools/sandbox/index.d.ts",
      "import": "./dist/tools/sandbox/index.js",
      "default": "./dist/tools/sandbox/index.js"
    },
    "./tools/sandbox/e2b": {
      "types": "./dist/tools/sandbox/e2b.d.ts",
      "import": "./dist/tools/sandbox/e2b.js",
      "default": "./dist/tools/sandbox/e2b.js"
    },
    "./tools/sandbox/sandock": {
      "types": "./dist/tools/sandbox/sandock.d.ts",
      "import": "./dist/tools/sandbox/sandock.js",
      "default": "./dist/tools/sandbox/sandock.js"
    },
    "./tools/browser": {
      "types": "./dist/tools/browser/index.d.ts",
      "import": "./dist/tools/browser/index.js",
      "default": "./dist/tools/browser/index.js"
    },
    "./tools/browser/browseruse": {
      "types": "./dist/tools/browser/browseruse.d.ts",
      "import": "./dist/tools/browser/browseruse.js",
      "default": "./dist/tools/browser/browseruse.js"
    },
    "./tools/browser/aws-agentcore": {
      "types": "./dist/tools/browser/aws-agentcore.d.ts",
      "import": "./dist/tools/browser/aws-agentcore.js",
      "default": "./dist/tools/browser/aws-agentcore.js"
    },
    "./tools/planning": {
      "types": "./dist/tools/planning/index.d.ts",
      "import": "./dist/tools/planning/index.js",
      "default": "./dist/tools/planning/index.js"
    },
    "./tools/memory": {
      "types": "./dist/tools/memory/index.d.ts",
      "import": "./dist/tools/memory/index.js",
      "default": "./dist/tools/memory/index.js"
    },
    "./tools/memory/mem0": {
      "types": "./dist/tools/memory/mem0.d.ts",
      "import": "./dist/tools/memory/mem0.js",
      "default": "./dist/tools/memory/mem0.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "benchmark": "tsx benchmark/run.ts",
    "benchmark:test": "tsx benchmark/run.ts --test",
    "benchmark:resume": "tsx benchmark/run.ts --resume",
    "benchmark:level1": "tsx benchmark/run.ts --level 1",
    "benchmark:level2": "tsx benchmark/run.ts --level 2",
    "benchmark:level3": "tsx benchmark/run.ts --level 3",
    "benchmark:files": "tsx benchmark/run.ts --category files",
    "benchmark:code": "tsx benchmark/run.ts --category code",
    "benchmark:search": "tsx benchmark/run.ts --category search",
    "benchmark:browser": "tsx benchmark/run.ts --category browser",
    "benchmark:reasoning": "tsx benchmark/run.ts --category reasoning",
    "benchmark:quick": "tsx benchmark/run.ts --limit 5 --verbose",
    "benchmark:random": "tsx benchmark/run.ts --random --verbose",
    "benchmark:wrong": "tsx benchmark/run-wrong-answers.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "autonomous",
    "gaia",
    "benchmark",
    "llm",
    "tools",
    "ai-sdk",
    "toolloop",
    "tree-shaking",
    "typescript"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gaia-agent/gaia-agent"
  },
  "packageManager": "pnpm@10.15.1",
  "sideEffects": false,
  "dependencies": {
    "@ai-sdk/openai": "beta",
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-bedrock-agentcore": "^3.928.0",
    "@aws-sdk/signature-v4": "^3.374.0",
    "@aws-sdk/signature-v4-multi-region": "^3.928.0",
    "@aws-sdk/util-format-url": "^3.922.0",
    "@duckdb/node-api": "1.4.1-r.5",
    "@huggingface/hub": "^2.6.12",
    "@smithy/protocol-http": "^5.3.5",
    "@smithy/signature-v4": "^5.3.5",
    "@tavily/core": "^0.5.12",
    "ai": "beta",
    "browser-use-sdk": "^2.0.4",
    "dotenv": "^17.2.3",
    "e2b": "^2.6.3",
    "exa-js": "^2.0.0",
    "hyparquet": "^1.20.2",
    "mime-types": "^3.0.1",
    "playwright": "^1.56.1",
    "sandock": "^0.2.4",
    "steel-sdk": "^0.14.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/mime-types": "^3.0.1",
    "@types/node": "24.6.2",
    "@vitest/ui": "^4.0.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.8"
  }
}
